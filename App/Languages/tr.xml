        <min_size>:_label dosya boyutu :_rule_value KB'dan büyük olmalıdır</min_size>
        <width>:_label genişliği :_rule_value px olmalıdır</width>
        <height>:_label yüksekliği :_rule_value px olmalıdır</height>
        <extensions>:_label uzantısı :_rule_value uzantılarından biri olmalıdır</extensions>
    </validation>
    <API>
